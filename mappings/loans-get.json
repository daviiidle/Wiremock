{
  "request": {
    "method": "GET",
    "urlPattern": "/loans/([a-zA-Z0-9]+)"
  },
  "response": {
    "status": 200,
    "headers": {
      "Content-Type": "application/json",
      "X-Correlation-Id": "{{request.headers.X-Correlation-Id}}"
    },
    "bodyFileName": "loan-{{request.pathSegments.[1]}}.json",
    "transformers": ["response-template"]
  }
}